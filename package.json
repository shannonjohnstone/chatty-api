{
  "name": "socket-chat-app",
  "version": "1.0.0",
  "description": "Socket IO chat appliation",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=latest",
    "compile-frontend": "babel --no-babelrc ./public/js/index.js -w -o ./public/dist/app.js --presets=latest,stage-0 --plugins=transform-object-rest-spread",
    "start-server": "nodemon --exec npm run babel-node -- ./server",
    "start-dev": "npm-run-all --parallel compile-frontend start-server",
    "start": "babel-node ./server"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "author": "Shannon Johnstone",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2"
  }
}
